<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">

    <fest:template xmlns:fest="http://fest.mail.ru" context_name="json">

        <div class="b-user__user left">
            <img src="{json.user.avatar}" class="b-user__user__ava left"/>
            <div class="b-user__user__name left">
                <fest:value output="text">json.user.first_name</fest:value>
                <fest:space/>
                <fest:value output="text">json.user.last_name</fest:value>
            </div>
        </div>

        <div class="b-user__exit right js-toolbar-exit">
            <div class="b-user__exit__text left">
                Выйти
            </div>
            <img src="/img/exit.png" class="b-user__exit__icon left"/>
        </div>

    </fest:template>

</fest:template>
